{
  "name": "fastify-tenant-control",
  "type": "module",
  "main": "./src/main.js",
  "bin": "./src/main.js",
  "scripts": {
    "build": "bun build ./src/main.js --outdir ./build --target bun --format esm --splitting --minify",
    "build:exe": "bun build ./src/main.js --minify --outfile ./bin/main --compile",
    "start": "bun run ./build/main.js",
    "start:exe": "./bin/main",
    "dev": "bun --watch ./src/main.js",
    "lint": "biome format . && biome check .",
    "lint:fix": "biome format --write . && biome check --apply .",
    "biome:migrate-prettier": "biome migrate prettier --write;",
    "biome:migrate-eslint": "biome migrate eslint --write --include-inspired;",
    "db:generate-migration": "drizzle-kit generate",
    "db:migrate": "bun run ./migrate.js",
    "db:push": "drizzle-kit push",
    "db:pull: ": "drizzle-kit introspect"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@trpc/server": "^10.45.2",
    "argon2": "^0.40.3",
    "drizzle-orm": "^0.30.10",
    "eslint": "^9.3.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "fastify": "^4.27.0",
    "fastify-guard": "^2.0.0",
    "fastify-zod": "^1.4.0",
    "jsonwebtoken": "^9.0.2",
    "ksuid": "^3.0.0",
    "pg": "^8.11.5",
    "pino": "^9.1.0",
    "pino-pretty": "^11.1.0",
    "trpc-openapi": "^1.2.0",
    "typia": "^6.0.4",
    "zennv": "^0.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.13.0",
    "@types/pg": "^8.11.6",
    "drizzle-kit": "^0.21.4",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "zod-to-json-schema": "^3.23.0"
  }
}
